{
  "name": "worklooking",
  "version": "1.0.0",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run vite\" \"wait-on http://localhost:5173 && nodemon\"",
    "vite": "vite",
    "build:main": "vite build --config vite.main.config.ts",
    "build:renderer": "vite build",
    "build": "npm run build:renderer && npm run build:main && electron-builder",
    "preview": "vite preview"
  },
  "build": {
    "appId": "com.worklooking.agent",
    "productName": "WorkLookingAgent",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "electron/agent/agent.md",
      "electron/themes/**/*",
      "package.json"
    ],
    "win": {
      "target": "portable",
      "icon": "electron/icon.png",
      "artifactName": "${productName}.${ext}"
    }
  },
  "dependencies": {
    "@openai/agents": "^0.3.9",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "handlebars": "^4.7.8",
    "jsonresume-theme-modern-sidebar": "file:electron/themes/modern-sidebar",
    "jsonresume-theme-spartan-fr": "file:electron/themes/spartan-fr",
    "lucide-react": "^0.562.0",
    "moment": "^2.30.1",
    "openai": "^6.16.0",
    "pdf-parse": "^2.4.5",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.12.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "wait-on": "^9.0.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/handlebars": "^4.0.40",
    "@types/node": "^25.0.9",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}

    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "framer-motion": "^12.26.2",
    "handlebars": "^4.7.8",
    "jsonresume-theme-modern-sidebar": "file:electron/themes/modern-sidebar",
    "jsonresume-theme-spartan": "^0.3.0",

{
  "name": "worklooking",
  "version": "1.0.1",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run vite\" \"npm run watch:main\" \"wait-on http://localhost:5173 dist-electron/main.js && nodemon\"",
    "vite": "vite",
    "watch:main": "vite build --config vite.main.config.ts --watch --mode development",
    "build:main": "vite build --config vite.main.config.ts",
    "build:renderer": "vite build",
    "build": "npm run build:renderer && npm run build:main",
    "build:forge": "npm run build && electron-forge package",
    "preview": "vite preview",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "package:win": "electron-forge make --platform win32 --arch x64",
    "package:all": "electron-forge make",
    "publish": "electron-forge publish",
    "make": "npm run build && npm run package:win",
    "clean": "rm -r electron-dist && rm -r dist"
  },
  "author": "Aymeric Barakat",
  "description": "WorkLooking Agent - AI-powered resume and job application assistant",
  "homepage": "./",
  "dependencies": {
    "@openai/agents": "^0.3.9",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron-squirrel-startup": "^1.0.1",
    "framer-motion": "^12.29.0",
    "handlebars": "^4.7.8",
    "lucide-react": "^0.562.0",
    "moment": "^2.30.1",
    "openai": "^6.16.0",
    "pdf-parse": "^2.4.5",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.12.0",
    "sharp": "^0.34.5",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "update-electron-app": "^3.1.2",
    "wait-on": "^9.0.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-deb": "^7.11.1",
    "@electron-forge/maker-rpm": "^7.11.1",
    "@electron-forge/maker-squirrel": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.11.1",
    "@electron-forge/plugin-fuses": "^7.11.1",
    "@electron-forge/plugin-vite": "^7.11.1",
    "@electron-forge/publisher-github": "^7.11.1",
    "@electron/fuses": "^1.8.0",
    "@types/handlebars": "^4.0.40",
    "@types/node": "^25.0.9",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/sharp": "^0.31.1",
    "@vitejs/plugin-react": "^5.1.2",
    "electron": "^40.0.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
